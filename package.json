{
    "name": "l-ui",
    "version": "1.0.0",
    "title": "l-ui",
    "private": true,
    "description": "A UI Library for Vue.",
    "main": "dist/lui.js",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "start": "DEBUG=l-ui* NODE_ENV=default node --abort-on-uncaught-exception app.js",
        "test": "DEBUG=* NODE_ENV=default node --abort-on-uncaught-exception app.js",
        "test:development": "DEBUG=* NODE_ENV=development node app.js",
        "test:stage": "DEBUG=* NODE_ENV=stage node app.js",
        "server": "set NODE_ENV=default && webpack-dev-server --inline --progress --open --hot",
        "build:development": "rimraf dist && NODE_ENV=development webpack --display-error-details  -p --progress  --config webpack.config.dev.js",
        "build:stage": "rimraf dist && NODE_ENV=stage webpack --display-error-details  -p --progress  --config webpack.config.stage.js",
        "build:production": "rimraf dist && NODE_ENV=production webpack --display-error-details  -p --progress  --config webpack.config.prod.js",
        "build:docs:development": "rimraf docs && NODE_ENV=development webpack --display-error-details  -p --progress  --config webpack.config.docs.dev.js",
        "publish": "git checkout dev && git pull && npm run build:development && git add . && git commit -m 'Publishtodevelopment' && git push && npm run server",
        "publish:stage": "git checkout stage && git merge dev && git pull && npm run build:stage && git add . && git commit -m 'Publish to stage' && git push",
        "publish:production": "git checkout production && git pull && git merge dev && npm run build:production && git add . && git commit -m 'Publish to production' && git push && git checkout dev",
        "pm2-start": "DEBUG=l-ui* NODE_ENV=default PM2_HOME=.pm2 pm2 start app.js -i max --name=l-ui",
        "pm2-stop": "PM2_HOME=.pm2 pm2 stop all",
        "pm2-restart": "PM2_HOME=.pm2 pm2 restart all",
        "pm2-status": "PM2_HOME=.pm2 pm2 status",
        "pm2-monit": "PM2_HOME=.pm2 pm2 monit",
        "pm2-start:development": "DEBUG=l-ui* NODE_ENV=development PM2_HOME=.pm2 pm2 start app.js -i 1 --name=l-ui",
        "pm2-stop:development": "PM2_HOME=.pm2 pm2 stop all",
        "pm2-restart:development": "PM2_HOME=.pm2 pm2 restart all",
        "pm2-start:stage": "DEBUG=l-ui* NODE_ENV=stage PM2_HOME=.pm2 pm2 start app.js -i max --name=l-ui",
        "pm2-stop:stage": "PM2_HOME=.pm2 pm2 stop all",
        "pm2-restart:stage": "PM2_HOME=.pm2 pm2 restart all"
    },
    "repository": {
        "type": "git",
        "url": "http://gitlab.lenovopcsd.com/h5/L-ui.git"
    },
    "keywords": [
        "L-ui",
        "ui",
        "components"
    ],
    "author": "zhuxj4 <zhuxj4@lenovo.com>",
    "license": "LENOVO",
    "bugs": {
        "url": "http://gitlab.lenovopcsd.com/h5/L-ui/issues"
    },
    "homepage": "http://gitlab.lenovopcsd.com/h5/L-ui",
    "dependencies": {
        "config": "^1.29.4",
        "debug": "^3.1.0",
        "glob": "^7.1.2",
        "ioredis": "^3.2.2",
        "koa": "^2.5.0",
        "koa-bodyparser": "^4.2.0",
        "koa-qs": "^2.0.0",
        "koa-router": "^7.4.0",
        "koa-session2": "^2.2.5",
        "koa-static": "^4.0.2",
        "l-ui": "git+http://gitlab.lenovopcsd.com/h5/l-ui.git#dev"
    },
    "engines": {
        "node": ">= 6.9.1",
        "npm": ">= 3"
    },
    "devDependencies": {
        "@types/webpack-env": "1.13.5",
        "appcache-webpack-plugin": "1.3.0",
        "awesome-typescript-loader": "^3.4.1",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^7.1.2",
        "babel-plugin-component": "^1.1.0",
        "babel-plugin-dynamic-import-node": "^1.2.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-es5-property-mutators": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "compression-webpack-plugin": "^1.1.11",
        "copy-webpack-plugin": "4.1.1",
        "css-loader": "0.28.7",
        "element-resize-detector": "^1.1.14",
        "eslint": "^3.19.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-config-vue": "^2.0.2",
        "eslint-friendly-formatter": "^3.0.0",
        "eslint-loader": "^1.7.1",
